{
    "name": "mpenna/laravel-specification",
    "description": "Implementation of the specification pattern",
    "keywords": ["laravel", "specification", "specification pattern"],
    "homepage": "https://github.com/chalcedonyt/laravel-specification",
    "license": "MIT",
    "authors": [{
        "name": "Tim Teoh",
        "email": "chalcedonyt@gmail.com",
        "role": "Developer"
    }],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "~5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "replace": {
        "chalcedonyt/laravel-specification": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Chalcedonyt\\Specification\\": "src"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "stable"
}
